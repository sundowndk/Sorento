<Addin id          = "sBlog"              
       author      = "Rasmus Pedersen"
       copyright   = "MIT X11"
       url         = "http://www.sundown.dk/"
       description = "Full feature Blog."	   
       version     = "1.0">
	<Runtime>
		<Import assembly="sBlog.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0"/>
		<Addin id="sCMS" version="1.0"/>
	</Dependencies>
		
	<Extension path = "/SorentoLib/Resolver/Renders">
		<Type type="sBlog.Addin.Render"/>
	</Extension>				
	
	<Extension path = "/SorentoLib/Resolver/Ajaxes">
		<Type type="sBlog.Addin.Ajax"/>
	</Extension>			
</Addin>