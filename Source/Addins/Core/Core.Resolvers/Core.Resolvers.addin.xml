<Addin id          = "Core.Resolvers"              
       author      = "Rasmus Pedersen"
       copyright   = "MIT X11"
       url         = "http://www.sundown.dk/"
       description = "Provides core functionality to the Sorento render engine."
	   category    = "Resolvers"
       version     = "1.0">
	<Runtime>
		<Import assembly="Core.Resolvers.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0" />
	</Dependencies>
	
	<Extension path = "/SorentoLib/Resolver/Renders">
		<Type type="Core.Resolvers.Addin.Render"/>
	</Extension>	
	
	<Extension path = "/SorentoLib/Resolver/Functions">
		<Type type="Core.Resolvers.Addin.Function"/>
	</Extension>		

	<Extension path = "/SorentoLib/Resolver/Ajaxes">
		<Type type="Core.Resolvers.Addin.Ajax"/>
	</Extension>				
</Addin>