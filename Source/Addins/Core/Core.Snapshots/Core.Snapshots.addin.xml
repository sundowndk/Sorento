<Addin id          = "Core.Snapshots"
       author      = "Rasmus Pedersen"
       copyright   = "MIT X11"
       url         = "http://www.sundown.dk/"
       description = "Provides core functionality to the Sorento render engine."
	   category    = "Resolvers"
       version     = "1.0">
	<Runtime>
		<Import assembly="Core.Snapshots.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0" />
	</Dependencies>
	
	<Extension path = "/SorentoLib/Services/Snapshots">
		<Type type="Core.Snapshots.Addin.Snapshot"/>
	</Extension>		
</Addin>

